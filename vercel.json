{
  "functions": {
    "backend/main.py": {
      "runtime": "python3.10",
      "memory": 3008,
      "maxDuration": 120
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "OMP_NUM_THREADS": "1",
    "MKL_NUM_THREADS": "1",
    "OPENBLAS_NUM_THREADS": "1",
    "NUMEXPR_NUM_THREADS": "1",
    "NEXT_PUBLIC_API_URL": "/api"
  }
}